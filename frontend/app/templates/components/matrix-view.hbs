<div id="matrix-view" style="width:100%; height: 100%">
    {{panel/left-panel
        displayData=displayData
        loadBlock="loadBlock"
        removeBlock="removeBlock"
        selectBlock="selectBlock"
    }}
    {{#if noData}}
        <div class="well">
            <span>Select individuals to display using Dataset Explorer</span>
        </div>
    {{/if}}
    <div id="observational-table"></div>
    <span id="length_checker" style="position:absolute; visibility: hidden"></span>
</div>
